#include<stdio.h>  
 int main()    
{    
int no,r,s=0,temp;    
printf("enter the number:");    
scanf("%d",&no);    
temp=no;    
while(no>0)    
{    
r=no%10;    
s=s+(r*r*r);    
no=no/10;    
}    
if(temp==s)    
printf("armstrong  number ");    
else    
printf("not armstrong number");    
return 0;  
}   
